	sendchat("/heal " id " 1")
}else{
sendchat("/med")
Sleep, 500
sendchat("/medhelp " id " 1")
Sleep, 500
}
return
GuiClose:
{
	ExitApp
}
return
reloadd:
{
	reload
}
return
