dwAddress := dwSAMP + 0x12D8F8
chatInput := readString(hGTA, dwAddress, 512)
if chatInput contains /mzreg
{
	if (org=Министерство здравоохранения) {
		RegExMatch(name, "(.*)_(.*)", result)
		addChatMessageEx(Color1, "" Color1 "[MZ Helper]:{3399FF} " result1 " " result2 "" Color2 ", Вы уже зарегистрированы в скрипте. ")
		addChatMessageEx(Color1, "" Color1 "[MZ Helper]:" Color2 " Для обновления данных введите /mztryreg ")
		return
	}else{
		goto, regest
	}	
}
else if chatInput contains /mztryreg
{
	goto, regest
}
else if chatInput contains /mzinfo
{
	if (org="Министерство здравоохранения") {
		fileread, info, *P65001 %DIRPRAVILA%info.txt
		sleep, 100
		ShowDialog("0", "{ffcc00}Информация о скрипте", info ,"Отмена")
		sleep, 100
	}else{
	addChatMessageEx(0xff0000, "{ff0000}[Ошибка]: " Color2 "Вы не сотрудник " Color1 " Министерство Здравоохранения" Color2 ". Если это не так, то зарегистрируйтесь в программе /mzreg")
	}
}
else if chatInput contains /hel
{
	if (org="Министерство здравоохранения") {
		RegExMatch(chatInput, "/hel (.*)", id)
		if (id1="") {
			addChatMessageEx(Color1, "" Color1 "[MZ Helper]:" Color2 " Используйте " Color1 "/hel [ID]")
		}else{
			goto, lechenie
		}
	}else{
	addChatMessageEx(0xff0000, "{ff0000}[Ошибка]: " Color2 "Вы не сотрудник " Color1 " Министерство Здравоохранения" Color2 ". Если это не так, то зарегистрируйтесь в программе /mzreg")
	}
}
else if chatInput contains /rn
{
	if (org="Министерство здравоохранения") {
		RegExMatch(chatInput, "/rn (.*)", text)
		if (text1="") {
			addChatMessageEx(Color1, "" Color1 "[MZ Helper]:" Color2 " Используйт," Color1 "/rn Текст " Color2 " Чтоб написать нонРП сообщение")
		}else{
			sendchat("/r (( " text1 " ))")
		}
	}else{
	addChatMessageEx(0xff0000, "{ff0000}[Ошибка]: " Color2 "Вы не сотрудник " Color1 " Министерство Здравоохранения" Color2 ". Если это не так, то зарегистрируйтесь в программе /mzreg")
	}
}
