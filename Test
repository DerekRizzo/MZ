timeget:
{
	url := "http://mvd.czar-script.ru/time.php"
	request := ComObjCreate("Microsoft.XMLHTTP")
	request.open("POST", url, false)
	try
	request.send()
	ms_utc := request.responseText
	RegExMatch(ms_utc, "(.*):(.*):(.*)<br>", clock)
	timetog:=clock1*60*60+(clock2*60)
}
return
clc:
{
	RegExMatch(name, "(.*)_(.*)",nameout)
	SendChat("/me " vzglanul " на золотые часы, с гравировкой '" nameout2 " Family'")
	Sleep 500
	SendChat("/c 60")
}
return
